{
  "compilerOptions": {
    "module": "ES2015",
    "target": "es5",
    "sourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true
  },
  "exclude": [
    "node_modules"
  ]
}